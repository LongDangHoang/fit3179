{
    "$schema": "https://vega.github.io/schema/vega/v5.json",
    "data": [
      {
        "name": "production",
        "url": "https://raw.githubusercontent.com/LongDangHoang/fit3179/master/data/processed/production_all_category_2000_2020.csv",
        "format": {"type": "csv"},
        "transform": [
          {
            "type": "stratify",
            "key": "id",
            "parentKey": "parent"
          },
          {
            "type": "treemap",
            "field": "size",
            "sort": {"field": "value"},
            "round": true,
            "method": "resquarify",
            "ratio": 1
          }
        ]
      }
    ],
  
    "layers": [
      {
  
      }
    ]
  }